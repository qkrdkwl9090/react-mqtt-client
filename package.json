{
  "name": "react-mqtt-hooks",
  "version": "1.0.0",
  "description": "A custom React Hooks library for MQTT",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build": "node esbuild.config.js",
    "prepare": "npm run build",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
